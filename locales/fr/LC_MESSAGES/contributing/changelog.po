msgstr ""

msgid "Addition: ``obj3 = (obj1 + obj2) * a + b``"
msgstr ""

msgid "Subtraction: ``obj3 = (obj1 - obj2) * a + b``"
msgstr ""

msgid "Multiplication: ``obj3 = (obj1 * obj2) * a + b``"
msgstr ""

msgid "Division: ``obj3 = (obj1 / obj2) * a + b``"
msgstr ""

