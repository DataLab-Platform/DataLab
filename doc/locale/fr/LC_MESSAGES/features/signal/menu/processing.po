#: ../../features/signal/menu/processing.rst:148
msgid "Equation"
msgstr ""

#: ../../features/signal/menu/processing.rst:149
msgid "Gaussian"
msgstr ""

#: ../../features/signal/menu/processing.rst:150
msgid ""
":math:`y = "
"y_{0}+\\dfrac{A}{\\sqrt{2\\pi}.\\sigma}.exp(-\\dfrac{1}{2}.(\\dfrac{x-x_{0}}{\\sigma})^2)`"
msgstr ""

#: ../../features/signal/menu/processing.rst:151
msgid "Lorentzian"
msgstr ""

#: ../../features/signal/menu/processing.rst:152
msgid ""
":math:`y = "
"y_{0}+\\dfrac{A}{\\sigma.\\pi}.\\dfrac{1}{1+(\\dfrac{x-x_{0}}{\\sigma})^2}`"
msgstr ""

#: ../../features/signal/menu/processing.rst:153
msgid "Voigt"
msgstr ""

#: ../../features/signal/menu/processing.rst:154
msgid ""
":math:`y = "
"y_{0}+A.\\dfrac{Re(exp(-z^2).erfc(-j.z))}{\\sqrt{2\\pi}.\\sigma}` with "
":math:`z = \\dfrac{x-x_{0}-j.\\sigma}{\\sqrt{2}.\\sigma}`"
msgstr ""

#: ../../features/signal/menu/processing.rst:155
msgid "Multi-Gaussian"
msgstr ""

#: ../../features/signal/menu/processing.rst:156
msgid ""
":math:`y = "
"y_{0}+\\sum_{i=0}^{K}\\dfrac{A_{i}}{\\sqrt{2\\pi}.\\sigma_{i}}.exp(-\\dfrac{1}{2}.(\\dfrac{x-x_{0,i}}{\\sigma_{i}})^2)`"
msgstr ""
