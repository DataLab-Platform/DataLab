#: ../../features/signal/menu/operation.rst:10
msgid "Sum"
msgstr ""

#: ../../features/signal/menu/operation.rst:7
msgid "Create a new signal which is the sum of all selected signals:"
msgstr ""

#: ../../features/signal/menu/operation.rst:9
msgid ""
"y_{M} = \\sum_{k=0}^{M-1}{y_{k}}\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:16
msgid "Average"
msgstr ""

#: ../../features/signal/menu/operation.rst:13
msgid "Create a new signal which is the average of all selected signals:"
msgstr ""

#: ../../features/signal/menu/operation.rst:15
msgid ""
"y_{M} = \\dfrac{1}{M}\\sum_{k=0}^{M-1}{y_{k}}\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:23
msgid "Difference"
msgstr ""

#: ../../features/signal/menu/operation.rst:19
msgid ""
"Create a new signal which is the difference of the **two** selected "
"signals:"
msgstr ""

#: ../../features/signal/menu/operation.rst:22
msgid ""
"y_{2} = y_{1} - y_{0}\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:29
msgid "Product"
msgstr ""

#: ../../features/signal/menu/operation.rst:26
msgid "Create a new signal which is the product of all selected signals:"
msgstr ""

#: ../../features/signal/menu/operation.rst:28
msgid ""
"y_{M} = \\prod_{k=0}^{M-1}{y_{k}}\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:35
msgid "Division"
msgstr ""

#: ../../features/signal/menu/operation.rst:32
msgid "Create a new signal which is the division of the **two** selected signals:"
msgstr ""

#: ../../features/signal/menu/operation.rst:34
msgid ""
"y_{2} = \\dfrac{y_{1}}{y_{0}}\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:41
msgid "Absolute value"
msgstr ""

#: ../../features/signal/menu/operation.rst:38
msgid "Create a new signal which is the absolute value of each selected signal:"
msgstr ""

#: ../../features/signal/menu/operation.rst:40
msgid ""
"y_{k} = |y_{k-1}|\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:47
msgid "Real part"
msgstr ""

#: ../../features/signal/menu/operation.rst:44
msgid "Create a new signal which is the real part of each selected signal:"
msgstr ""

#: ../../features/signal/menu/operation.rst:46
msgid ""
"y_{k} = \\Re(y_{k-1})\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:53
msgid "Imaginary part"
msgstr ""

#: ../../features/signal/menu/operation.rst:50
msgid "Create a new signal which is the imaginary part of each selected signal:"
msgstr ""

#: ../../features/signal/menu/operation.rst:52
msgid ""
"y_{k} = \\Im(y_{k-1})\n"
"\n"
msgstr ""

#: ../../features/signal/menu/operation.rst:62
msgid "Convert data type"
msgstr ""

#: ../../features/signal/menu/operation.rst:56
msgid ""
"Create a new signal which is the result of converting data type of each "
