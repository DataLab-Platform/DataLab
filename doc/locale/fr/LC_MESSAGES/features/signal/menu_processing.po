msgid "Area"
msgstr "Aire"

msgid ":math:`y_{1}= \\dfrac{y_{0}}{\\sum_{n=0}^{N}y_{0}[n]}`"
msgstr ""

msgid "Energy"
msgstr "Energie"

msgid ":math:`y_{1}= \\dfrac{y_{0}}{\\sqrt{\\sum_{n=0}^{N}|y_{0}[n]|^2}}`"
msgstr ""

msgid "RMS"
