msgstr ""

#, python-brace-format
msgid "``{index}``: 1-based index of the signal in the selection (with zero-padding)"
msgstr ""

#, python-brace-format
msgid "``{count}``: Total number of selected signals"
msgstr ""

#, python-brace-format
msgid "``{xlabel}``, ``{xunit}``, ``{ylabel}``, ``{yunit}``: Axis labels and units"
msgstr ""

#, python-brace-format
msgid "``{metadata[key]}``: Access metadata values"
msgstr ""

